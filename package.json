{
  "name": "eang",
  "version": "0.2.6",
  "description": "Reactive Angular component library",
  "author": "Sebastian Kropp",
  "license": "MIT",
  "main": "./dist/eang.umd.js",
  "module": "./dist/eang.es5.js",
  "es2015": "./dist/eang.js",
  "typings": "./dist/eang.d.ts",
  "scripts": {
    "start": "cd demo && ng serve",
    "build": "npm run clean && npm run build:es5 && npm run build:es2015 && npm run build:umd && npm run copy",
    "build:watch": "npm run build && onchange \"src/**/*\" -- npm run build",
    "build:es5": "rimraf build && ngc -p src/tsconfig.es5.json && rollup -c rollup-config.js -i ./build/eang.es5.js -o ./dist/eang.es5.js",
    "build:es2015": "rimraf build && ngc -p src && rollup -c rollup-config.js",
    "build:umd": "rimraf build && ngc -p src/tsconfig.es5.json && rollup -c rollup-config.umd.js -i build/eang.es5.js -o dist/eang.umd.js",
    "copy": "cpx \"build/**/!(*.js)\" dist",
    "lint": "ng lint",
    "prepublish": "npm run build",
    "postpublish": "git push && git push --tags",
    "clean": "rimraf build && rimraf dist"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5",
    "markdown-it": "^8.3.2"
  },
  "devDependencies": {
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "codelyzer": "^3.1.2",
    "cpx": "^1.5.0",
    "cross-var": "^1.0.3",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.2",
    "onchange": "^3.2.1",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.2.2",
    "uglify-js": "^3.0.27",
    "zone.js": "^0.8.5"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/eascientific/eang.git"
  },
  "bugs": {
    "url": "https://github.com/eascientific/eang/issues"
  },
  "homepage": "https://github.com/eascientific/eang"
}
