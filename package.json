{
  "name": "eang",
  "version": "0.1.2",
  "description": "Reactive Angular component library",
  "author": "Sebastian Kropp",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "build": "npm run ngc && npm run rollup && npm run copy",
    "build:watch": "npm run build && onchange \"src/**/*\" -- npm run build",
    "ngc": "ngc -p src/tsconfig.json",
    "rollup": "rollup -c rollup-config.fesm.js && rollup -c rollup-config.fesm.js",
    "copy": "cpx \"build/**/!(*.js)\" dist && cpx src/package.json dist && cpx README.md dist",
    "lint": "ng lint",
    "postversion": "cd src && cross-var npm version $npm_package_version",
    "prepublish": "cross-var git commit -m \"d3 ${npm_package_version}\" && npm run build",
    "postpublish": "git push && git push --tags"
  },
  "devDependencies": {
    "@angular/common": "^4.3.2",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "codelyzer": "^3.1.2",
    "cpx": "^1.5.0",
    "cross-var": "^1.0.3",
    "onchange": "^3.2.1",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.2.2",
    "uglify-js": "^3.0.27",
    "zone.js": "^0.8.5"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/eascientific/eang.git"
  },
  "bugs": {
    "url": "https://github.com/eascientific/eang/issues"
  },
  "homepage": "https://github.com/eascientific/eang"
}
